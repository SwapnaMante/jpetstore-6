pipeline {

agent any

environment {
   
    ArtifactID = readMavenPom().getArtifactId()
    VERSION = readMavenPom().getVersion()
    }
stages {
        
        stage ('Prep') {
            steps {
                checkout scm
            }
		}
  stage('Print') {
            steps {
				echo "${ArtifactID}"
                echo "${VERSION}"
            }

        }	
stage('Build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
		
}
}